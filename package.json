{
  "name": "time-speak",
  "version": "1.8.0",
  "description": "A natural language parser for dates, times and intervals.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/f3rno64/time-speak",
  "author": "Cris Mihalache <f3rno64@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/f3rno64/time-speak",
  "bugs": {
    "url": "https://github.com/f3rno64/time-speak/issues"
  },
  "keywords": [
    "natural language",
    "parser",
    "date",
    "time",
    "relative",
    "date parser"
  ],
  "scripts": {
    "docs": "npx typedoc --out docs src && cp LICENSE.md docs/LICENSE.md && cp CHANGELOG.md docs/CHANGELOG.md",
    "docs:md": "npm run build && rm -rf DOCUMENTATION.md && ./node_modules/.bin/jsdoc2md -f 'dist/**/*.js' > DOCUMENTATION.md",
    "gen-readme": "npm run build && rm README.md && node scripts/gen_readme.js",
    "test": "NODE_PATH=./src NODE_ENV=test mocha",
    "test:coverage": "NODE_PATH=./src NODE_ENV=test nyc mocha",
    "test:watch": "NODE_PATH=./src NODE_ENV=test nyc mocha --watch",
    "build": "NODE_PATH=./src tsc -p tsconfig.json",
    "lint": "eslint -f unix \"src/**/*.{ts,tsx}\"",
    "update-deps": "updates -u -g -c",
    "update-version": "standard-version -a",
    "prepare": "husky install",
    "prepare-release": "npm run lint && npm run test && npm run build && npm run docs && git add docs && npm run docs:md && npm run gen-readme",
    "prepublish": "npm run build",
    "release": "npm run prepare-release && npm run update-version && git push --follow-tags origin main"
  },
  "devDependencies": {
    "@eslint/js": "^8.54.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.11",
    "@types/chai-almost": "^1.0.3",
    "@types/eslint__js": "^8.42.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.1",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "chai": "^4.3.10",
    "chai-almost": "^1.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "jsdoc-to-markdown": "^8.0.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3",
    "updates": "^15.0.4"
  },
  "dependencies": {
    "numbers-from-words": "^0.0.8",
    "tslib": "^2.6.2"
  }
}
