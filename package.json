{
  "name": "time-speak",
  "version": "1.3.0",
  "description": "A natural language parser for dates, times and intervals.",
  "main": "index.js",
  "repository": "https://github.com/f3rno64/time-speak",
  "author": "Cris Mihalache <f3rno64@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21",
    "numbered": "^1.1.0"
  },
  "packageManager": "yarn@4.0.2",
  "devDependencies": {
    "chai": "^4.3.10",
    "chai-almost": "^1.0.1",
    "eslint": "^8.54.0",
    "jsdoc-to-markdown": "^8.0.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "standard-version": "^9.5.0"
  },
  "scripts": {
    "docs": "rm -rf DOCUMENTATION.md && ./node_modules/.bin/jsdoc2md -f index.js 'lib/**/*.js' > DOCUMENTATION.md",
    "gen-readme": "rm README.md && node scripts/gen_readme.js",
    "test": "npm run lint && npm run unit",
    "lint": "./node_modules/.bin/eslint '*.js' lib test",
    "unit": "NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/mocha --recursive",
    "update-version": "./node_modules/.bin/standard-version"
  }
}
